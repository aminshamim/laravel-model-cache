{
    "name": "aminshamim/laravel-model-cache",
    "description": "A Laravel package for automatic model caching with performance optimization",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "cache", "model", "performance", "redis"],
    "authors": [
        {
            "name": "Amin Shamim",
            "email": "amin.shamim@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "ext-json": "*",
        "ext-redis": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5|^11.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10",
        "laravel/pint": "^1.13",
        "nunomaduro/collision": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "AminShamim\\LaravelModelCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AminShamim\\LaravelModelCache\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AminShamim\\LaravelModelCache\\ModelCacheServiceProvider"
            ],
            "aliases": {
                "ModelCache": "AminShamim\\LaravelModelCache\\Facades\\ModelCache"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
